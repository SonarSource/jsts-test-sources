{
    "name": "CourseLit LMS",
    "dockerfile": "Dockerfile",
    "settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	"forwardPorts": [
		3000,
		8000,
		27017
	],
	"dockerFile": "Dockerfile",
	"extensions": [
		"eamodio.gitlens",
		"ms-vscode.vscode-typescript-next",
		"wayou.vscode-todo-highlight"
	],
	"postAttachCommand": "startup.sh",
	"runArgs": [
		"--add-host=domain1.localsite.com:127.0.0.1",
		"--add-host=domain2.localsite.com:127.0.0.1",
		"--add-host=localsite.com:127.0.0.1"
	]
}